{
  "family": "mlops-task2",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::103702179512:role/mlopsecsTaskExecutionRole",
  "cpu": "1024",
  "memory": "3072",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "mlops-task2-demo",
      "image": "103702179512.dkr.ecr.us-east-2.amazonaws.com/my-fastapi-app:latest",
      "cpu": 0,
      "memory": null,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "environment": [],
      "environmentFiles": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mlops-task2",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": [],
      "ulimits": []
    }
  ]
}
